<div class="container">
  <%= month_calendar events: @schedules do |date, schedules| %>
    <%= date %>
  
    <% schedules.each do |schedule| %>
      <div>
        <%= schedule.name %>
      </div>
    <% end %>
  <% end %>
  <%= link_to '予定を入れる', new_schedule_path, class: "btn btn-primary" %>
</div>